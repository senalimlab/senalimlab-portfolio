---
import BaseLayout from "../../layouts/BaseLayout.astro";
const title = "Scaling Trusted, Sustainable Care";
---
<BaseLayout title={title} sideBarActiveItemID="projects">
  <section class="flex flex-col flex-1">
    <h2 class="heading-2 mb-6 text-center">Case Study: {title}</h2>
    <iframe
      src="https://gamma.app/embed/kw1gywhu6wz95c1"
      class="w-full flex-1"
      allow="fullscreen"
      title={title}
    ></iframe>
  </section>
</BaseLayout>
